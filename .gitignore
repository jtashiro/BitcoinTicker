local.properties
    gitignore
    # Built application files
    *.apk
    *.aab
    *.ap_
    *.aار # Android archive (Android ARchive)
    *.dex

    # Files for the ART/Dalvik VM
    *.dex

    # Java class files
    *.class

    # Generated files
    bin/
    gen/
    out/

    # Gradle files
    .gradle/
    build/

    # Local configuration files (SDK path, etc)
    local.properties

    # Log files
    *.log

    # Android Studio Navigation editor files
    .navigation/

    # Android Studio Vcs files
    .idea/vcs.xml
    .idea/workspace.xml # Often user-specific, but some teams commit it. Your choice.

    # JetBrains Rider specific files
    .idea/shelf/

    # Keystore files (NEVER commit these unless you have a specific, secure reason)
    *.jks
    *.keystore

    # External native build directory
    .cxx/

    # User-specific settings for Android Studio (these should generally be ignored)
    .idea/caches/
    .idea/gradle.xml
    .idea/migrations.xml
    .idea/libraries/
    .idea/modules.xml # Can sometimes be committed if everyone uses the same module structure
    .idea/runConfigurations/ # Usually user-specific
    .idea/misc.xml # Often contains user-specific paths or settings

    # Captured snapshots (for UI testing, etc. - usually not needed in repo)
    captures/

    # Lint results
    lint-results-*.xml
    lint-results-*.html

    # Heap dump files
    *.hprof

    # Sometimes these are generated by plugins or tools
    gen-external-apklibs/

    # CMake intermediate files
    cmake-build-*/
    
